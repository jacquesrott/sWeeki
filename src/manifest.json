{
  "name": "sWeeki",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Make Wikipedia feel sweeter.",
  "icons": {
    "16":   "icons/icon16.png",
    "48":   "icons/icon48.png",
    "128":  "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*.wikipedia.org/*"],
      "css": ["style.css"]
    }
  ],
  "permissions": ["tabs", "http://*.wikipedia.org/*"]
}